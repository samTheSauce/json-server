{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "db/db.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "api/server.js": {
            "memory": 1024,
            "includeFiles": "db/db.json"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "server.js"
        }
    ]
}